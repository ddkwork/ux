  <entry type="NameVariableClass" style="italic #6fafbd"/> <!-- 类变量 -->
  <entry type="NameVariableGlobal" style="italic #6fafbd"/> <!-- 全局变量 -->
  <entry type="NameVariableInstance" style="italic #6fafbd"/> <!-- 实例变量 -->
  <entry type="Literal" style="#a9b7c6"/> <!-- 字面量 -->
