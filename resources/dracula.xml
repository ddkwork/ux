  <entry type="Background" style="bg:#2b2b2b"/> <!-- 背景 -->
  <entry type="Keyword" style="#c7773e"/> <!-- 关键字 -->
  <entry type="KeywordConstant" style="#97727F"/> <!-- 关键字常量 -->
  <entry type="KeywordDeclaration" style="italic #CB652E"/> <!-- 关键字声明 -->
  <entry type="KeywordNamespace" style="#c7773e"/> <!-- 命名空间关键字 -->
  <entry type="KeywordPseudo" style="#c7773e"/> <!-- 伪关键字 -->
  <entry type="KeywordReserved" style="#c7773e"/> <!-- 保留关键字 -->
  <entry type="KeywordType" style="#CB7730"/> <!-- 类型关键字 -->
  <entry type="Name" style="#a9b7c6"/> <!-- 名称 -->
  <entry type="NameAttribute" style="#4eade5"/> <!-- 名称属性 -->
  <entry type="NameBuiltin" style="italic #6fafbd"/> <!-- 内置名称 -->
  <entry type="NameBuiltinPseudo" style="#AEBE7B"/> <!-- 内置伪名称 -->
  <entry type="NameClass" style="#4eade5"/> <!-- 类名称 -->
  <entry type="NameConstant" style="#97727F"/> <!-- 常量名称 -->
  <entry type="NameEntity" style="#a9b7c6"/> <!-- 实体名称 -->
  <entry type="NameException" style="#a9b7c6"/> <!-- 异常名称 -->
  <entry type="NameFunction" style="#B09D79"/> <!-- 函数名称 -->
  <entry type="NameLabel" style="italic #6fafbd"/> <!-- 标签名称 -->
