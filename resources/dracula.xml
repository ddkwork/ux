  <entry type="GenericInserted" style="bold #4eade5"/> <!-- 插入标记 -->
  <entry type="GenericOutput" style="#44475a"/> <!-- 输出 -->
  <entry type="GenericPrompt" style="#a9b7c6"/> <!-- 提示 -->
  <entry type="GenericStrong" style="#a9b7c6"/> <!-- 强调文本 -->
  <entry type="GenericSubheading" style="bold #a9b7c6"/> <!-- 子标题 -->
  <entry type="GenericTraceback" style="#a9b7c6"/> <!-- 回溯 -->
  <entry type="GenericUnderline" style="underline"/> <!-- 下划线 -->
  <entry type="Text" style="#a9b7c6"/> <!-- 文本 -->
  <entry type="TextWhitespace" style="#a9b7c6"/> <!-- 空白文本 -->
</style>
